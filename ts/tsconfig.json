{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "moduleResolution": "node",
    "strict": true,
    "rootDir": "./src",
    "outDir": "../lua",
    "types": [
      "lua-types/5.3",
      "nvim-types/0.5.0-nightly"
    ]
  },
  "tstl": {
    "luaTarget": "JIT",
    "noImplicitSelf": true
  }
}